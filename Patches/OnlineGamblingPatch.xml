<?xml version="1.0" encoding="utf-8"?>
<!-- Patches the vanilla Comms Console to be used as the online gamble machine -->
<Patch>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="CommsConsole"]/thingClass</xpath>
    <value>
      <thingClass>RimGamble.NewCommsConsoleOnlineGambling</thingClass>
    </value>
  </Operation>
</Patch>