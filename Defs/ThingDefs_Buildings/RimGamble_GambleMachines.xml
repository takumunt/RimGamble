<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>RimGamble_SlotMachine</defName>
    <label>slot machine</label>
    <description>A machine that simulates gambling.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <drawSize>(2.4,2.4)</drawSize>
      <texPath>RimGamble/SlotMachine/SlotMachine</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shadowData>
        <volume>(0.5,0.35,0.4)</volume>
      </shadowData>
    </graphicData>
    <defaultPlacingRot>South</defaultPlacingRot>
    <fillPercent>0.30</fillPercent>
    <altitudeLayer>Building</altitudeLayer>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <passability>PassThroughOnly</passability>
    <pathCost>42</pathCost>
    <socialPropernessMatters>true</socialPropernessMatters>
    <building>
      <joyKind>Gaming_Cerebral</joyKind>
      <buildingSizeCategory>Medium</buildingSizeCategory>
    </building>
    <costList>
      <Steel>50</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <MarketValue>350</MarketValue>
      <Mass>60.0</Mass>
      <JoyGainFactor>1.3</JoyGainFactor>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Joy</designationCategory>
    <designationHotKey>Misc5</designationHotKey>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>MicroelectronicsBasics</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>
</Defs>